{
  "name": "@finp2p/core",
  "version": "1.0.0",
  "description": "FinP2P Core - Atomic swap transaction routing and processing between blockchains",
  "main": "dist/adapters/index.js",
  "types": "dist/adapters/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "ts-node --watch src/index.ts",
    "build": "tsc",
    "clean": "rimraf dist",
    "rebuild": "npm run clean && npm run build",
    "build:watch": "tsc --watch",
    "test:setup": "node testing/setup-test-redis.js setup",
    "test:teardown": "node testing/setup-test-redis.js cleanup",
    "test:find-port": "node testing/setup-test-redis.js find-port",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest testing/tests/integration",
    "test:unit": "jest tests/unit",
    "test:adapters": "jest tests/adapters",
    "test:router": "jest tests/router",
    "test:security": "jest testing/tests/security",
    "test:performance": "jest testing/tests/performance",
    "test:compliance": "jest testing/tests/compliance",
    "test:reliability": "jest testing/tests/reliability",
    "test:solutions": "jest testing/tests/solutions",
    "test:real-adapters": "jest --config testing/tests/solutions/real-adapters/jest.real-adapters.config.js",
    "test:axelar": "npx ts-node testing/axelar-adapter-test.ts",
    "test:all": "jest testing/tests",
    "test:confirmation": "jest tests/confirmation",
    "benchmark": "npx ts-node testing/benchmark/benchmark-comprehensive-domains.ts",
    "benchmark:dissertation": "npm run build && node testing/benchmark/benchmark-comprehensive-domains.js",
    "benchmark:empirical": "npm run build && node scripts/run-empirical-benchmark.js",
    "benchmark:empirical:direct": "npm run build && node testing/benchmark/empirical-benchmark.js",
    "benchmark:test-performance": "npm run build && node testing/benchmark/test-performance-benchmark.js",
    "benchmark:test-performance:detailed": "npm run build && node testing/benchmark/test-performance-benchmark.js --detailed",
    "benchmark:test-performance:stress": "npm run build && node testing/benchmark/test-performance-benchmark.js --stress",
    "test:no-docker": "jest --detectOpenHandles --forceExit",
    "test:no-integration": "jest --testPathIgnorePatterns=integration --testPathIgnorePatterns=adapters",
    "lint": "eslint adapters/**/*.ts core/**/*.ts",
    "lint:fix": "eslint adapters/**/*.ts core/**/*.ts --fix",
    "format": "prettier --write adapters/**/*.ts core/**/*.ts",
    "type-check": "tsc --noEmit",
    "demo:atomic-swap": "npm run build && node demos/finp2p/finp2p-atomic-swap-coordination-demo.js",
    "demo:adapters": "npm run build && node demos/finp2p/hedera-sui-adapters-demo.js",
    "demo:fusion": "npm run build && node demos/finp2p/fusion-multi-chain-demo.js",
    "demo:fusion-pure": "npm run build && node demos/pure/fusion-pure-demo.js",
    "demo:fusion-spec": "npm run build && node demos/fusion/fusion-adapters-demo.js",
    "demo:axelar": "npm run build && node demos/axelar/axelar-adapter-demo.js",
    "demo:axelar:ts": "npx ts-node demos/axelar/axelar-adapter-demo.ts",
    "demo:axelar:true-atomic-swap": "npx ts-node demos/axelar/axelar-true-atomic-swap-demo.ts",
    "demo:axelar:simple-swap": "npx ts-node demos/axelar/axelar-simple-swap-demo.ts",
    "demo:layerzero": "npm run build && node demos/layerzero/layerzero-adapter-demo.js",
    "demo:layerzero:ts": "npx ts-node demos/layerzero/layerzero-adapter-demo.ts",
    "demo:layerzero-router": "npm run build && node layerzero-router/layerzero-router-demo.js",
    "demo:layerzero-router:ts": "npx ts-node layerzero-router/layerzero-router-demo.ts",
    "benchmark:axelar": "npx ts-node results/axelar/benchmark-test/axelar-criteria-benchmark.ts",
    "benchmark:axelar:run": "bash results/axelar/benchmark-test/run-axelar-benchmark.sh",
    "benchmark:axelar:run:win": "results\\axelar\\benchmark-test\\run-axelar-benchmark.bat",
    "demo:ccip": "npm run build && node demos/ccip/ccip-adapter-demo.js",
    "demo:ccip:ts": "npx ts-node demos/ccip/ccip-adapter-demo.ts",
    "demo:frontend": "node frontend/server.js",
    "demo:start": "npm run build && node frontend/server.js",
    "docker:build": "docker build -t finp2p-router .",
    "docker:run": "docker run -p ${PORT:-3000}:${PORT:-3000} finp2p-router",
    "compose:up": "docker-compose -f docker/docker-compose.yml up -d",
    "compose:down": "docker-compose -f docker/docker-compose.yml down",
    "compose:logs": "docker-compose -f docker/docker-compose.yml logs -f",
    "compose:build": "docker-compose -f docker/docker-compose.yml build"
  },
  "keywords": [
    "finp2p",
    "blockchain",
    "atomic swap",
    "dlt",
    "sui",
    "hedera",
    "routing",
    "financial",
    "p2p"
  ],
  "author": "Alexander Ryan Plumbridge <ucabrpl@ucl.ac.uk>",
  "license": "MIT",
  "dependencies": {
    "@axelar-network/axelarjs-sdk": "^0.17.4",
    "@chainlink/contracts": "^1.4.0",
    "@chainlink/contracts-ccip": "^1.6.0",
    "@cosmjs/cosmwasm-stargate": "^0.36.0",
    "@cosmjs/proto-signing": "^0.36.0",
    "@cosmjs/stargate": "^0.36.0",
    "@hashgraph/hethers": "^1.2.6",
    "@hashgraph/sdk": "^2.71.1",
    "@hyperledger/fabric-gateway": "^1.8.0",
    "@hyperledger/fabric-protos": "^0.3.7",
    "@layerzerolabs/create-oapp": "^0.0.148",
    "@layerzerolabs/devtools": "^2.0.1",
    "@layerzerolabs/io-devtools": "^0.3.1",
    "@layerzerolabs/lz-core": "^3.0.126",
    "@layerzerolabs/lz-definitions": "^3.0.126",
    "@layerzerolabs/lz-foundation": "^3.0.126",
    "@layerzerolabs/lz-sdk": "^0.0.87",
    "@layerzerolabs/lz-utilities": "^3.0.126",
    "@layerzerolabs/oapp-evm": "^0.3.2",
    "@layerzerolabs/oft-evm": "^3.2.1",
    "@layerzerolabs/protocol-devtools-evm": "^5.0.0",
    "@layerzerolabs/scan-client": "^0.0.8",
    "@layerzerolabs/solidity-examples": "^1.1.0",
    "@layerzerolabs/ua-devtools": "^5.0.0",
    "@mysten/bcs": "^1.6.3",
    "@mysten/sui": "^1.33.0",
    "@owneraio/finp2p-sdk-js": "^0.24.2",
    "@wormhole-foundation/sdk-evm-core": "REMOVED",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "ethers": "^6.8.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "redis": "^4.6.10",
    "socket.io": "^4.7.5",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.1",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
